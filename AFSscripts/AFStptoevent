if game.PlaceId ~= 16946008847 then return end
local tp_state = false
if not getgenv().TpToEventStatement then return end
repeat task.wait() until game:IsLoaded()
while #game:GetService("Workspace").Characters:GetChildren() ~= getgenv().AmountOfAccounts do
    task.wait(1)
    print("Waiting till other alts join")
end
while task.wait(1) do
    for _,v in pairs(game:GetService("Workspace"):FindFirstChild("Characters"):GetDescendants()) do
        if v:IsA("Model") and v.Name == 'HumanoidRootPart' and #game:GetService("Workspace").Characters:GetChildren() == AmountOfAccounts then
            local rep, suc = pcall(function()
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-10, 256.6, -112)
            end)
            if suc then 
                print("Teleported to event Successfully!")
            end
        end
    end
end
